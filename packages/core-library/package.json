{
  "name": "core-library",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/core-library/core-library.esm.js",
  "exports": {
    ".": {
      "import": "./dist/core-library/core-library.esm.js",
      "require": "./dist/core-library/core-library.cjs.js"
    },
    "./dist/*": {
      "import": "./dist/*",
      "types": "./dist/*"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test.watch": "stencil test --spec --watchAll",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e": "npx playwright test",
    "generate": "stencil generate",
    "storybook:dev": "npm run build && storybook dev -p 6006",
    "storybook:build": "npm run build && storybook build"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@stencil/core": "^4.27.1",
    "@stencil/playwright": "^0.2.1",
    "@stencil/react-output-target": "^1.1.0",
    "@stencil/vue-output-target": "^0.10.8",
    "@storybook/addon-a11y": "^9.0.18",
    "@storybook/web-components-vite": "^9.0.18",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lit": "^3.3.1",
    "puppeteer": "^24.3.0",
    "storybook": "^9.0.18"
  },
  "license": "MIT",
  "dependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@floating-ui/dom": "^1.7.2",
    "@stencil/sass": "^3.2.1",
    "@storybook/addon-docs": "^9.0.18",
    "uuid": "^11.1.0",
    "vite": "^7.0.6"
  }
}
